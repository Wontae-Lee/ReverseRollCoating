FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



xminFluid  -0.02;
xmaxFluid  0.02;
yminFluid  -0.02;
ymaxFluid  0.02;
zminFluid  0.0;
zmaxFluid  0.01;




defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    boxToCell
    {
        box ($xminFluid $yminFluid $zminFluid) ($xmaxFluid $ymaxFluid $zmaxFluid);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);


// ************************************************************************* //
