FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



xminFluid  -0.05;
xmaxFluid  0.05;
yminFluid  0.25;
ymaxFluid  0.30;
zminFluid  -0.02;
zmaxFluid  0.02;




defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    boxToCell
    {
        box ($xminFluid $yminFluid $zminFluid) ($xmaxFluid $ymaxFluid $zmaxFluid);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);


// ************************************************************************* //
